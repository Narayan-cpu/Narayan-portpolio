{
    "builds": [{
        "src": "Narayannaik/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Narayannaik/wsgi.py"
        }

    ],
  "functions": [  {
    "api/**/*.py": {
      "maxDuration": 60
    }
  ],
      
    
}
